{
  "name": "md-agenda",
  "version": "0.1.0",
  "description": "A Node.js CLI program that parses markdown files for deadlines and displays them.",
  "main": "./bin/index.js",
  "bin": {
    "mdag": "./bin/index.js"
  },
  "scripts": {
    "start": "nodemon --watch 'src/**/*.ts' --exec 'ts-node' src/index.ts",
    "build": "tsc -p .",
    "local": "npm i -g && mdag",
    "refresh": "rm -rf ./node_modules ./package-lock.json && npm install",
    "lint": "eslint . --ext .ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dennisschoepf/md-agenda.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/dennisschoepf/md-agenda/issues"
  },
  "homepage": "https://github.com/dennisschoepf/md-agenda#readme",
  "devDependencies": {
    "@types/glob": "^7.1.3",
    "@types/node": "^14.14.7",
    "@typescript-eslint/eslint-plugin": "^4.7.0",
    "@typescript-eslint/parser": "^4.7.0",
    "@types/fs-extra": "^9.0.4",
    "eslint": "^7.13.0",
    "nodemon": "^2.0.6",
    "prettier": "^2.1.2",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.5"
  },
  "dependencies": {
    "glob": "^7.1.6",
    "gray-matter": "^4.0.2",
    "luxon": "^1.25.0"
  }
}
